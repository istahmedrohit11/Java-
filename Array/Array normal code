import java.util.Scanner;
class HelloWorld {
    public static void main(String args[]){
        int [] marks= new int[5] ;
        // declaration  //Memory allocation
        //or
        //int []marks={10,20,30,40,50};
        marks[0]=100;
        marks[1]=200;
        marks[2]=300;
        int i;
        for(i=0;i<5;i++){
            System.out.println(marks[i]);
        }
       }
    } 


