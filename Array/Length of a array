import java.util.Scanner;
class HelloWorld {
    public static void main(String args[]){  
        int []marks={10,20,30,40,50};
        
        int i;
        System.out.println("The length of the array is: " +marks.length);
        for(i=0;i<marks.length;i++){
            System.out.println(marks[i]);
        }
        
       }
    } 


