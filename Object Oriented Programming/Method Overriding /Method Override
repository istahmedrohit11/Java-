class Base1{
    public void x1(){
        System.out.println("Method one of Base1");
    }
}
class Base2 extends Base1{
    @Override
    public void x1(){
        System.out.println("Method one of Base2");
    }
    public void x2(){
        System.out.println("Method two of Base2");
    }
}
public class Hello{
    public static void main(String[] args){
      // Base1 b=new Base1();
       //b.x1();
       Base2 c=new Base2();
       c.x2();
    }
}
