
 class Employee{
    int id;
    String name;
    long salary;
    //method
    public void printDetails(){
        System.out.println("My id is:"+id);
        System.out.println("My name is:"+name);
        System.out.println("MY Salary:"+salary);
    }
    public long getSalary(){
        return salary;
    }
}
public class Hello{
    public static void main(String args[]){
        System.out.println("this is my custom class");
        Employee rohit = new Employee();
        Employee ronaldo= new Employee();
        //setting attributes
        rohit.id=12;
        rohit.name="Istiaque Ahmed";
        rohit.salary=120000;

        ronaldo.id=40;
        ronaldo.name="Suiii";
        ronaldo.salary=9999999999L;

        // System.out.println(rohit.id);
        // System.out.println(rohit.name);
        rohit.printDetails();
        System.out.println("my salary is"+rohit.getSalary());
        ronaldo.printDetails();
        System.out.println("my salary is"+ronaldo.getSalary());
    }
}
