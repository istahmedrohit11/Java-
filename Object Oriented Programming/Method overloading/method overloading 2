class Sum{
    public int display(int x,int y){
        return x+y;
    }
    public int display(double y,double x){
        return (int)(x+y);
    }
}
public class HelloWorld{
    public static void main(String args[]){
        Sum s= new Sum();
        int p=10,q=20,r=30;
        int a=s.display(p,q);
        int b=s.display(p,q);
        System.out.println("a+b="+a+"  "+"a+b="+b);


        
    }
}
