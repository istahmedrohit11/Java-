public class Hello {
    public static void main(String[] args) {
        int a=10;
        int b=0;
        // int c=a/b;
        // System.out.println(c);
        try{
            int c=a/b;
            System.out.println("The result is:"+c);
        }
        catch(Exception e){
            System.out.println("We failed to divided.reason: "+e);
        }
        System.out.println("End of the program");
    }
}
