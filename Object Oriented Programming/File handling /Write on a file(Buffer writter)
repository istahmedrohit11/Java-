import java.io.*;
public class Hello{
    public static void main(String[] args) {
      try{
        BufferedWriter bw=new BufferedWriter(new FileWriter("Rohit.txt",true));
        bw.write("Hello World------");
        bw.newLine();
        bw.close();
    }catch(Exception e){
        e.printStackTrace();
        }
    }
}
