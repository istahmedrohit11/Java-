abstract class Animal{
    Animal(){
        System.out.println("Animal constructor called without parameters");
    }
    Animal(String name){
        System.out.println("Animal constructor called with parameter: " + name);
    }
}
class Dog extends Animal{
    Dog(){
       // super();
        System.out.println("Dog constructor called without parameters");
    }
    Dog(String name){
        super(name);
        System.out.println("Dog constructor called with parameter: " + name);
    }
}
public class javacode{
    public static void main(String[] args){ 
       // Dog dog1 = new Dog(); // Calls the no-argument constructor of Dog and Animal
        Dog dog2 = new Dog("Buddy"); // Calls the parameterized constructor of Dog and Animal
    
   
    }
}
