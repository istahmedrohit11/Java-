// Tightly Coupled Example

class DieselEngine{ 
    public void Start(){    
        System.out.println("Diesel Engine Start");
    }
}

class Car{
    DieselEngine engine=new DieselEngine(); // Tightly coupled to DieselEngine
    public void StartCar(){
        engine.Start();
    }
}
public class TightlyCoupled{
    public static void main(String[] args){
    Car c1=new Car();
    c1.StartCar();
    }
}
