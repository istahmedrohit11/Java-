class DieselEngine {
    public void start() {
        System.out.println("Diesel Engine started.");
    }
}

class Car {
    DieselEngine engine = new DieselEngine();  // Tightly coupled to DieselEngine

    public void startCar() {
        engine.start();
    }
}

public class TightlyCoupledExample{
    public static void main(String[] args) {
        Car c1 = new Car();
        c1.startCar();
    }
}
